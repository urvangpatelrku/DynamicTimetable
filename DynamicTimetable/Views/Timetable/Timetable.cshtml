@model List<List<string>>

<table class="table table-bordered table-striped table-hover">
    <thead class="table-dark">
        <tr>
            @for (int i = 0; i < Model[0].Count; i++)
            {
                <th>Day @(@i + 1)</th>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model)
        {
            <tr>
                @foreach (var subject in row)
                {
                    <td>@subject</td>
                }
            </tr>
        }
    </tbody>
</table>
